.NOLOG
.JOB COPY ABORT=CANCEL
.SETC 6='.' 8='.'
/ INPUT LIBRARY OR "NEWLINE" FOR NONE
.ENTER 5
.SKIP 1 IF ? LE #5
.SETC 5='' 6=''
/ OUTPUT LIBRARY OR "NEWLINE" FOR NONE
.ENTER 7
.SKIP 1 IF ? LE #7
.SETC 7='' 8=''
.FSPEC #7#8#2 ON #3 9=SIZE
.SKIP 2 IF #9 GT 0
.FSPEC #5#6#0 ON #1 9=TYPE
.SKIP TO SEC IF #9 NE L
.NEW #7#8#2 ON #3 '#9' 1T
.SKIP 2
=SEC
.NEW #7#8#2 ON #3 '#9' 1S
.USE #5#6#0 ON #1 FOR SYS0
.USE #7#8#2 ON #3 FOR SYS1
.RUN S.XCOPUT
.END
S.CON

